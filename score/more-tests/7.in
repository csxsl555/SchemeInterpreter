(define (first-n-squares n)
  (define (helper i acc)
    (if (> i n)
        acc
        (helper (+ i 1) (cons (* i i) acc))))
  (helper 1 (list)))

(first-n-squares 4)

(define (list*2 ls)
  (if (null? ls)
      '()
      (cons (* 2 (car ls))
            (list*2 (cdr ls)))))

(define (list+1 ls)
  (if (null? ls)
      '()
      (cons (+ 1 (car ls))
            (list+1 (cdr ls)))))

(define (list-square ls)
  (if (null? ls)
      '()
      (cons (* (car ls) (car ls))
            (list-square (cdr ls)))))

(define (list-negate ls)
  (if (null? ls)
      '()
      (cons (- 0 (car ls))
            (list-negate (cdr ls)))))

(define (list-length ls)
  (if (null? ls)
      0
      (+ 1 (list-length (cdr ls)))))

(define (list-sum ls)
  (if (null? ls)
      0
      (+ (car ls) (list-sum (cdr ls)))))

(define (list-product ls)
  (if (null? ls)
      1
      (* (car ls) (list-product (cdr ls)))))

(define (list-max ls)
  (if (null? ls)
      'error
      (if (null? (cdr ls))
          (car ls)
          (let ((rest-max (list-max (cdr ls))))
            (if (> (car ls) rest-max)
                (car ls)
                rest-max)))))

(define (list-min ls)
  (if (null? ls)
      'error
      (if (null? (cdr ls))
          (car ls)
          (let ((rest-min (list-min (cdr ls))))
            (if (< (car ls) rest-min)
                (car ls)
                rest-min)))))

(define (list-reverse ls)
  (define (reverse-helper ls acc)
    (if (null? ls)
        acc
        (reverse-helper (cdr ls) (cons (car ls) acc))))
  (reverse-helper ls '()))

(define (filter-positive ls)
  (if (null? ls)
      '()
      (if (> (car ls) 0)
          (cons (car ls) (filter-positive (cdr ls)))
          (filter-positive (cdr ls)))))

(define (list-contains? ls x)
  (if (null? ls)
      #f
      (if (= (car ls) x)
          #t
          (list-contains? (cdr ls) x))))

(define (list-ref ls n)
  (if (null? ls)
      'error
      (if (= n 0)
          (car ls)
          (list-ref (cdr ls) (- n 1)))))

(define (list-append ls1 ls2)
  (if (null? ls1)
      ls2
      (cons (car ls1) (list-append (cdr ls1) ls2))))

