(define (list*2 ls)
  (if (null? ls)
      (list)   
      (cons (* 2 (car ls))   
            (list*2 (cdr ls))))) 


(define (list+1 ls)
  (if (null? ls)
      (list)
      (cons (+ 1 (car ls))
            (list+1 (cdr ls)))))


(define (list-sum ls)
  (if (null? ls)
      0   
      (+ (car ls) (list-sum (cdr ls)))))

(define (list-length ls)
  (if (null? ls)
      0   
      (+ 1 (list-length (cdr ls)))))  


(define (reverse-helper ls acc)
  (if (null? ls)
      acc    
      (reverse-helper (cdr ls) (cons (car ls) acc))))  


(define (list-reverse ls)
  (reverse-helper ls (list)))  

